{
  "name": "rich-hi-res",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "./rich-hi-res/src",
  "tags": [],
  "targets": {
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "svelte-check",
        "cwd": "./rich-hi-res"
      }
    },
    "add": {
      "executor": "@nxext/sveltekit:add"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": ["./rich-hi-res/**/*.{ts,svelte,spec.ts}"]
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/rich-hi-res"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "rich-hi-res:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "rich-hi-res:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "rich-hi-res:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "rich-hi-res:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "rich-hi-res:build:development"
        },
        "production": {
          "buildTarget": "rich-hi-res:build:production"
        }
      }
    }
  }
}
